<h1>Listing lesson_learneds</h1>

<table>
  <tr>
    <th>Lesson Date</th>
    <th>Project Reference</th>
    <th>Project Engineer</th>
    <th>Project Architect</th>
    <th>Project Owner</th>
    <th>Division</th>
    <th>Input</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lesson_learneds.each do |lesson_learned| %>
  <tr>
    <td><%= lesson_learned.lessonDate %></td>
    <td><%= lesson_learned.projectReference %></td>
    <td><%= lesson_learned.projectEngineer %></td>
    <td><%= lesson_learned.projectArchitect %></td>
    <td><%= lesson_learned.projectOwner %></td>
    <td><%= lesson_learned.division %></td>
    <td><%= lesson_learned.input %></td>
    <td><%= link_to 'Show', lesson_learned %></td>
    <td><%= link_to 'Edit', edit_lesson_learned_path(lesson_learned) %></td>
    <td><%= link_to 'Destroy', lesson_learned, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

